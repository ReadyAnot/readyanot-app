{
  "name": "ready-anot-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "cz": "cz",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "generate": "prisma generate && graphql-codegen --config codegen.yml",
    "deploy": "ansible-playbook ansible/main.yml",
    "studio-dev": "dotenv -e .env.development.local -- prisma studio",
    "studio-prod": "dotenv -e .env.production.local -- prisma studio",
    "db-push-dev": "dotenv -e .env.development.local -- prisma db push --preview-feature",
    "db-push-dev-force": "dotenv -e .env.development.local -- prisma db push --preview-feature --force",
    "db-push-prod": "dotenv -e .env.production.local -- prisma db push --preview-feature",
    "db-push-prod-force": "dotenv -e .env.production.local -- prisma db push --preview-feature --force",
    "setup": "ansible-playbook ansible/setup.yml"
  },
  "dependencies": {
    "@apollo/client": "^3.3.6",
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@prisma/client": "^2.13.1",
    "apollo-server-micro": "^2.19.1",
    "aws-sdk": "^2.821.0",
    "graphql": "^15.4.0",
    "graphql-iso-date": "^3.6.1",
    "graphql-tag": "^2.11.0",
    "next": "10.0.4",
    "react": "17.0.1",
    "react-dom": "17.0.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.20.0",
    "@graphql-codegen/introspection": "1.18.1",
    "@graphql-codegen/typescript": "1.20.0",
    "@graphql-codegen/typescript-operations": "1.17.13",
    "@graphql-codegen/typescript-react-apollo": "2.2.1",
    "@graphql-codegen/typescript-resolvers": "1.18.1",
    "@prisma/cli": "^2.14.0",
    "@types/graphql-iso-date": "^3.4.0",
    "@types/node": "^14.14.16",
    "@types/react": "^17.0.0",
    "commitizen": "^4.2.2",
    "dotenv-cli": "^4.0.0",
    "typescript": "^4.1.3"
  }
}
